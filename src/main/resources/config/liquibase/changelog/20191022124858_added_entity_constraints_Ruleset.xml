<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Ruleset.
    -->
    <changeSet id="20191022124858-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ruleset_id"
                                 baseTableName="ruleset_game"
                                 constraintName="fk_ruleset_game_ruleset_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ruleset"/>

        <addForeignKeyConstraint baseColumnNames="game_id"
                                 baseTableName="ruleset_game"
                                 constraintName="fk_ruleset_game_game_id"
                                 referencedColumnNames="id"
                                 referencedTableName="game"/>
        
    </changeSet>
</databaseChangeLog>
